<h1>Sign In</h1>
<h2>Url: /sessions/new</h2>

<%= form_with url:sessions_path, local: true do |f| %>
    
    <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div>

    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>

    <%= f.submit 'Sign In' %>

<% end %>


